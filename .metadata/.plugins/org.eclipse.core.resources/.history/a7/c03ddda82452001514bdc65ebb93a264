
public class Star {

	public static void main(String[] args) {
// TODO Auto-generated method stub
		int i=0,j=0;
		while(i<=6)
		{
			while(j<=6)
			{
				if(i+j>=6)
				{
				System.out.print("*");
				
			}
		j++;
			}
		i++;
		j=1;
		}
		else
		{
			
		
		
			System.out.println(" ");
		}
		}

	}


