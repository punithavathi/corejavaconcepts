
public class Star {

	public static void main(String[] args) {
// TODO Auto-generated method stub
		int i=1,j=1;
		while(i<6)
		{
			while(j<6)
			{
				if(i+j>6)
				{
				System.out.print("*");
				
			}
		
			
			else
			{
				
			
			
				System.out.println(" ");
			}
		j++;
			}
			i++;
			j=1;
		
		
		}

	}


}